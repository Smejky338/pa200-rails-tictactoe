{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */1 * * * *"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "admin",
      "collectionName": "games",
      "createIfNotExists": false,
      "connectionStringSetting": "CosmosDbConnectionString"
    }
  ]
}